{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "browser":{
    "child_process": false
  },
  "scripts": {
    "start:dev": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.js",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode production --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@khanacademy/react-multi-select": "^0.3.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "module": "^1.2.5",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-multi-carousel": "^2.5.2",
    "react-notifications": "^1.5.0-beta.0",
    "react-notifications-component": "^2.3.0",
    "react-quill": "^1.3.3",
    "react-responsive-gallery": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-share": "^4.0.1",
    "reactstrap": "^8.4.1",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {}
}
